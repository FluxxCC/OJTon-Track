{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/config.js", "destination": "/api/index.js" },
    { "source": "/assets/(.*)", "destination": "/api/index.js" },
    { "source": "/icons/(.*)", "destination": "/api/index.js" },
    { "source": "/favicon.ico", "destination": "/api/index.js" },
    { "source": "/favicon.png", "destination": "/api/index.js" }
  ]
}
